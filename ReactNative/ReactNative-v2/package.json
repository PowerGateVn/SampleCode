{
  "name": "reactnative-tutorial",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "eslint src/**",
    "clean-android": "cd ./android&& gradlew clean",
    "build-android": "react-native run-android --variant=release",
    "build-ios": "node node_modules/react-native/local-cli/cli.js bundle --entry-file ./index.js --bundle-output ./ios/main.jsbundle --dev false --platform ios --assets-dest ./ios"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "lodash": "^4.17.10",
    "native-base": "^2.7.2",
    "react": "^16.4.2",
    "react-native": "^0.55.4",
    "react-native-vector-icons": "^5.0.0",
    "react-navigation": "^2.10.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.0",
    "remote-redux-devtools": "^0.5.12",
    "remotedev-rn-debugger": "^0.8.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-preset-react-native": "^4.0.0",
    "eslint": "^5.2.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-react-native": "^3.2.1",
    "jest": "^23.4.2",
    "react-test-renderer": "^16.4.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
